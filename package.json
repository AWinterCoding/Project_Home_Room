  "bugs": {
    "url": "https://github.com/AWinterCoding/Project_Home_Room/issues"
  },
  "homepage": "https://github.com/AWinterCoding/Project_Home_Room#readme",
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.7",
    "concurrently": "^8.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^10.15.0",
    "jest": "^29.6.2",
    "mongoose": "^7.4.1",
    "react": "^18.2.0"
  }
}
